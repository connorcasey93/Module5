   "source": [
    "# Calculate the final tumor volume of each mouse across four of the treatment regimens\n",
    "\n",
    "# Start by getting the last (greatest) timepoint for each mouse\n",
    "\n",
    "select_drugs = ['Capomulin', 'Ramicane', 'Infubinol', 'Ceftamin']\n",
    "select_drugs = clean_mouse[clean_mouse['Drug Regimen'].isin(select_drugs)]\n",
    "\n",
    "# Find the last (greatest) timepoint for each mouse within the selected treatment regimens\n",
    "last_time = select_drugs.groupby('Mouse ID')['Timepoint'].max()\n",
    "\n",
    "\n",
    "# Merge this group df with the original DataFrame to get the tumor volume at the last timepoint\n",
    "final_volume = pd.merge(select_drugs,last_time, on=['Mouse ID', 'Timepoint'])\n",
    "final_volume"
   ]
  },
