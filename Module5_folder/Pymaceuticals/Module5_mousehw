    "# Calculate the correlation coefficient and a linear regression model\n",
    "# for mouse weight and average observed tumor volume for the entire Capomulin regimen\n",
    "\n",
    "correlation_mouse=round(st.pearsonr(capomulin_mouse['Weight (g)'],capomulin_mouse['Tumor Volume (mm3)'])[0],2)\n",
    "print(f\"The correlation between mouse weight and average tumor volume is {correlation_mouse}\")\n",
    "\n",
    "capo_slope, capo_int, capo_r, capo_p, capo_std_err = st.linregress(capomulin_mouse[\"Weight (g)\"],capomulin_mouse[\"Tumor Volume (mm3)\"])\n",
    "capo_fit = capo_slope * capomulin_mouse[\"Weight (g)\"] + capo_int\n",
    "plt.scatter(capomulin_mouse[\"Weight (g)\"],capomulin_mouse[\"Tumor Volume (mm3)\"],color=\"b\")\n",
    "plt.plot(capomulin_mouse[\"Weight (g)\"],capo_fit,\"--\",color=\"r\")\n",
    "plt.xlabel(\"Weight (g)\")\n",
    "plt.ylabel(\"Average Tumor Volume (mm3)\")\n",
